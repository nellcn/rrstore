<li id="<%= feed_item.id %>">
  <span class="prod_id">Prod ID: <%= feed_item.prod_id %>  </span>
  <span class="prod_name">Product Name: TBD  </span>
  <span class="prod_price">Product Price:  </span>
  <span class="prod_count">Count: <%= feed_item.prod_count %></span>
  <% if current_user?(feed_item.user) %>
    <%= link_to "delete", feed_item, method: :delete, data: { confirm: "You sure?" } %>
  <% end %>
</li>
